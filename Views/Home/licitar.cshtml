@model final.Models.Licitacoes
@{
    ViewData["Title"] = "Efectuar Licitacoes";
}

<h1>@ViewData["Title"]</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="licitador" class="control-label"></label>
                <input asp-for="licitador" class="form-control" />
                <span asp-validation-for="licitador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="valorLicitado" class="control-label"></label>
                <input asp-for="valorLicitado" class="form-control" />
                <span asp-validation-for="valorLicitado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="veiculosParaVendaId" class="control-label"></label>
                <select asp-for="veiculosParaVendaId" class ="form-control" asp-items="ViewBag.veiculosParaVendaId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

